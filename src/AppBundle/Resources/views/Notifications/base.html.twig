{% import "@App/Notifications/macros.html.twig" as macros %}

{% if menu %}
    {% block menu_preview %}
        {{ macros.menu_preview(notification, title, content) }}
    {% endblock %}
{% elseif preview %}
    {% block preview %}
        {{ macros.preview(notification, title, content) }}
    {% endblock %}
{% else %}
    {% block complete %}
        {{ macros.complete(notification, title, content) }}
    {% endblock %}
{% endif %}

