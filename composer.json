{
    "name": "gonzague/unmutebe",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": ">=3.3.6",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^3.0",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "friendsofsymfony/user-bundle": "dev-master",
        "symfony/event-dispatcher": ">=2.8",
        "stripe/stripe-php": "4.*",
        "symfony/assetic-bundle": "^2.8",
        "sonata-project/admin-bundle": "^3.20",
        "sonata-project/doctrine-orm-admin-bundle": "^3.1",
        "tetranz/select2entity-bundle": "2.*",
        "jmose/command-scheduler-bundle": "^1.2",
        "leafo/scssphp": "^0.6.7",
        "patchwork/jsqueeze": "^2.0",
        "sonata-project/translation-bundle": "^2.1",
        "jms/i18n-routing-bundle": "^2.0",
        "jms/translation-bundle": "^1.3",
        "twbs/bootstrap": "4.0.0-beta",
        "spipu/html2pdf": "^5.0",
        "ramsey/uuid": "3.7.0",
        "yellowskies/QR-code-bundle": "^1.2",
        "azine/email-bundle": "dev-master",
        "ninsuo/symfony-collection": "^2.1",
        "a2lix/translation-form-bundle": "^2.1",
        "knplabs/doctrine-behaviors": "^1.4",
        "oneup/uploader-bundle": "^1.7",
        "sonata-project/doctrine-extensions": "^1.0",
        "vich/uploader-bundle": "^1.6",
        "craue/formflow-bundle": "~3.0",
        "fp/jsformvalidator-bundle": "^1.5",
    },
    "scripts": {
        "post-install-cmd": [
            "Fuz\\Symfony\\Collection\\ScriptHandler::postInstall"
        ],
        "post-update-cmd": [
            "Fuz\\Symfony\\Collection\\ScriptHandler::postUpdate"
        ]
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "jms/di-extra-bundle": "^1.8",
        "deployer/deployer": "~3.0"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.0"
        }
    }
}
