{% extends 'base.html.twig' %}

{% block stylesheets %}
    {% stylesheets filter='cssrewrite' output='css/compiled/catalogue.min.css'
        'jplist/jplist.css'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {{ parent() }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="//cdnjs.cloudflare.com/ajax/libs/babel-polyfill/6.26.0/polyfill.min.js"></script>

    {% javascripts filter='jsqueeze' output='js/compiled/catalogue.js'
        'jplist/jplist.js'
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}